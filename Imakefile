DEFINES=-I$(AFSDIR)/include

#ifdef SOLARIS
OSLIBS=-lucb
#endif
#ifdef sgi
OSLIBS=-lsys
#endif

AFSLIBS = -L$(AFSDIR)/lib -L$(AFSDIR)/lib/afs -lsys -lrx -llwp
LIBS=-lknet -lkrb $(AFSDIR)/lib/libdes.a -lhesiod -lcom_err $(OSLIBS)

SimpleProgram(newpag,newpag.o,$(AFSLIBS) $(LIBS),$(ATHBINDIR))
